{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@Home/*": ["Home/*"],
      "@Collaboration/*": ["Collaboration/*"],
      "@Trade/*": ["Trade/*"],
      "@Chatting/*": ["Chatting/*"],
      "@ArtRoom/*": ["ArtRoom/*"],
      "@Search/*": ["Search/*"],
      "@WorkSharing/*": ["WorkSharing/*"],
      "@FreeBoard/*": ["FreeBoard/*"],
      "@Notification/*": ["Notification/*"],
      "@Comments/*": ["Comments/*"]
    }
  },
  "include": [
    "src/**/*",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
